{
  "compilerOptions": {
    "module": "commonjs",
    "target": "ES2019",
    "outDir": "out",
    "lib": [
      "ES2019"
    ],
    "sourceMap": true,
    "rootDir": ".",
    "noImplicitAny": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "noImplicitReturns": true,
    "noImplicitThis": true,
  },
  "exclude": [
    "node_modules",
    ".vscode-test"
  ]
}
